<% if(typeof  errors != 'undefined') { %>
    <% errors.forEach(errs => { %>
            <div class="alert alert-warning alert-dismissable fade show" role="alert" >
                <%= errs %>
                <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>

    <% }) %>

<%} %>